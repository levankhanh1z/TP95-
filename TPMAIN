local ReplicatedStorage = game:GetService("ReplicatedStorage")
local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

local curCapacity = ReplicatedStorage:WaitForChild("CurCapacity")
local framePosition = CFrame.new(-325.817078, 10075.7402, -153.631927, 1, 0, 0, 0, 1, 0, 0, 0, 1)

local function teleportToFrame()
    player.Character:Move(framePosition.Position)
end

-- Hàm kiểm tra giá trị CurCapacity và thực hiện teleport nếu thỏa mãn điều kiện
local function checkAndTeleport()
    if curCapacity.Value >= 114000 then
        teleportToFrame()

        -- Kiểm tra điều kiện và đợi giá trị đổi thành 0
        repeat
            wait(1)
        until curCapacity.Value == 0

        -- Lặp lại nếu cần thiết
        checkAndTeleport()
    end
end

-- Gọi hàm kiểm tra và teleport
checkAndTeleport()
